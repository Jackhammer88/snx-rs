sexpr      = _{ SOI ~ expr ~ EOI }
expr       = _{ "(" ~ command ~ data ~ ")" }
command    = { ident }
data       = _{ obj | array | value }
obj        = { field+ }
array      = { elem+ }
field      = { ":" ~ ident ~ "(" ~ data? ~ ")" }
elem       = { ":" ~ "(" ~ data? ~ ")" }
ident      = { (ASCII_ALPHANUMERIC | "_")+ }
char       = _{ !("(" | ")") ~ ANY }
value      = { char+ }
WHITESPACE = _{ " " | "\n" | "\r" | "\t" | "\x00" }
